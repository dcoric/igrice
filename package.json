{
  "name": "one-stop-shop",
  "version": "1.0.0-alpha",
  "description": "One Stop Shop",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "debug": "better-npm-run debug",
    "dev": "better-npm-run dev",
    "test": "mocha --compilers js:babel-core/register --require ./tests/test_helper.js --require ignore-styles --recursive ./tests",
    "test:watch": "npm run test -- --watch",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "devbuild": "better-npm-run deploy:dev",
    "qabuild": "better-npm-run deploy:qa",
    "prodbuild": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:qa": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "qa",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "npm run lint:fix && node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "debug": {
      "command": "npm run lint:fix && node --debug-brk bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcoric/react-redux-starter-kit.git"
  },
  "author": "Anonymous",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "better-npm-run": "0.0.15",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "1.6.2",
    "css-loader": "0.28.0",
    "cssnano": "3.10.0",
    "debug": "2.6.6",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "font-awesome": "4.7.0",
    "fs-extra": "3.0.0",
    "html-webpack-plugin": "2.28.0",
    "ip": "1.1.5",
    "json-loader": "0.5.4",
    "node-sass": "4.5.2",
    "normalize.css": "6.0.0",
    "postcss-loader": "1.3.3",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "4.1.1",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "yargs": "7.1.0",
    "zip-webpack-plugin": "1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "3.0.1",
    "express": "4.15.2",
    "jquery": "3.2.1",
    "jsdom": "10.0.0",
    "mocha": "3.3.0",
    "react-addons-test-utils": "15.5.1",
    "redbox-react": "1.3.6",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  }
}
